[user]
    name = Kevin yang
    email = yangke@umich.edu

[core]
    editor = vim

[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[alias]
    lo = log --oneline
